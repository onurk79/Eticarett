@model IEnumerable<Eticarett.Models.Urunler>


<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>List</title>
</head>
<body>
    <p>
        @Html.ActionLink("New", "New")
    </p>
    <table class="table">
        <tr>
            
            <th>
                @Html.DisplayNameFor(model => model.Katagori.KategoriAdi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Markalar.MarkaAdi)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Acıklama)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
           
            <td>
                @Html.DisplayFor(modelItem => item.Katagori.KategoriAdi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Markalar.MarkaAdi)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Acıklama)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
                @Html.ActionLink("Details", "Details", new { id=item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.Id })
            </td>
        </tr>
    }
    
    </table>
    @Html.ActionLink("Geri Dön", "List", "Homes")
</body>
</html>
