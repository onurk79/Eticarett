    @using (Html.BeginForm())
    {
        <div class="modal fade" id="GuvenlikSorusu" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel">
            <div class="modal-dialog" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                        <h4 class="modal-title" id="exampleModalLabel">Güncelle</h4>
                    </div>
                    <div class="modal-body">
                        <div class="form-group">
                            @Html.LabelFor(model => model.GuvenlikSorusu, htmlAttributes: new { @class = "control-label col-md-2" })
                            <div class="col-md-10">
                                @Html.TextBoxFor(x => x.GuvenlikSorusu, new { htmlAttributes = new { @class = "form-control" } })

                                @Html.ValidationMessageFor(model => model.GuvenlikSorusu, "", new { @class = "text-danger" })
                            </div>
                        </div>
